<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.madhouse.platform.premiummad.dao.AdvertiserMapper" >
  <select id="selectByAdvertiserKeyAndDspId" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from mad_sys_advertiser
    where advertiser_key = #{advertiserKey,jdbcType=VARCHAR}
      and dsp_id = #{dspId,jdbcType=INTEGER}
  </select>
  <insert id="insertAdvertiser" parameterType="com.madhouse.platform.premiummad.entity.Advertiser" 
          useGeneratedKeys="true" keyProperty="id">
    insert into mad_sys_advertiser
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        id,
      </if>
      <if test="advertiserKey != null" >
        advertiser_key,
      </if>
      <if test="advertiserName != null" >
        advertiser_name,
      </if>
      <if test="dspId != null" >
        dsp_id,
      </if>
      <if test="industry != null" >
        industry,
      </if>
      <if test="website != null" >
        website,
      </if>
      <if test="contact != null" >
        contact,
      </if>
      <if test="address != null" >
        address,
      </if>
      <if test="phone != null" >
        phone,
      </if>
      <if test="license != null" >
        license,
      </if>
      <if test="createdTime != null" >
        created_time,
      </if>
      <if test="updatedTime != null" >
        updated_time,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        #{id,jdbcType=INTEGER},
      </if>
      <if test="advertiserKey != null" >
        #{advertiserKey,jdbcType=VARCHAR},
      </if>
      <if test="advertiserName != null" >
        #{advertiserName,jdbcType=VARCHAR},
      </if>
      <if test="dspId != null" >
        #{dspId,jdbcType=INTEGER},
      </if>
      <if test="industry != null" >
        #{industry,jdbcType=SMALLINT},
      </if>
      <if test="website != null" >
        #{website,jdbcType=VARCHAR},
      </if>
      <if test="contact != null" >
        #{contact,jdbcType=VARCHAR},
      </if>
      <if test="address != null" >
        #{address,jdbcType=VARCHAR},
      </if>
      <if test="phone != null" >
        #{phone,jdbcType=VARCHAR},
      </if>
      <if test="license != null" >
        #{license,jdbcType=VARCHAR},
      </if>
      <if test="createdTime != null" >
        #{createdTime,jdbcType=TIMESTAMP},
      </if>
      <if test="updatedTime != null" >
        #{updatedTime,jdbcType=TIMESTAMP},
      </if>
    </trim>
  </insert>
</mapper>