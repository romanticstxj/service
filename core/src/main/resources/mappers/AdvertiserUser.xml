<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.madhouse.platform.premiummad.dao.AdvertiserUserMapper" >
  <select id="selectAdvertiserUser" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from mad_sys_advertiser_user
    where adverster_id = #{advertiserId,jdbcType=INTEGER}
    and user_id = #{userId,jdbcType=VARCHAR}
  </select>
  
  <insert id="insertAdvertiserUser" parameterType="com.madhouse.platform.premiummad.entity.AdvertiserUser"
  		  useGeneratedKeys="true" keyProperty="id" >
    insert into mad_sys_advertiser_user
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        id,
      </if>
      <if test="adversterId != null" >
        adverster_id,
      </if>
      <if test="userId != null" >
        user_id,
      </if>
      <if test="qualificationFile != null" >
        qualification_file,
      </if>
      <if test="status != null" >
        status,
      </if>
      <if test="reason != null" >
        reason,
      </if>
      <if test="createdTime != null" >
        created_time,
      </if>
      <if test="updatedTime != null" >
        updated_time,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        #{id,jdbcType=INTEGER},
      </if>
      <if test="adversterId != null" >
        #{adversterId,jdbcType=INTEGER},
      </if>
      <if test="userId != null" >
        #{userId,jdbcType=VARCHAR},
      </if>
      <if test="qualificationFile != null" >
        #{qualificationFile,jdbcType=VARCHAR},
      </if>
      <if test="status != null" >
        #{status,jdbcType=TINYINT},
      </if>
      <if test="reason != null" >
        #{reason,jdbcType=VARCHAR},
      </if>
      <if test="createdTime != null" >
        #{createdTime,jdbcType=TIMESTAMP},
      </if>
      <if test="updatedTime != null" >
        #{updatedTime,jdbcType=TIMESTAMP},
      </if>
    </trim>
  </insert>
</mapper>